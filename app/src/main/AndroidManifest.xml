<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="dwiyanhartono.com.applikasidestinasiwisata">

    <application
        android:allowBackup="true"
        android:icon="@drawable/woind4"
        android:label="@string/app_name"
        android:roundIcon="@drawable/woind4"
        android:supportsRtl="true"
        android:theme="@style/Theme.ApplikasiDestinasiWisata"
        tools:ignore="AllowBackup">
        <activity
            android:screenOrientation="portrait"
            android:name=".Profile"
            android:parentActivityName=".Halaman1"
            android:label="@string/labelprofile"/>
        <activity
            android:screenOrientation="portrait"
            android:name=".DetailActivity"
            android:label="Detail"
            android:parentActivityName=".Halaman1" />
        <activity
            android:screenOrientation="portrait"
            android:name=".Halaman1"
            android:label="Home" />
        <activity
            android:screenOrientation="portrait"
            android:name=".SplashScreen"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>